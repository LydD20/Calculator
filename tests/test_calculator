#going on but will keep looking into it.

from calculator import Official_Calculator

'''Test for Addition Function'''
def test_add():
    assert Official_Calculator.add(2,2)==4

'''Test for Subtraction Function'''
def test_subtract():
    assert Official_Calculator.subtract(2,1)==1

def test_multiply():
    assert Official_Calculator.multiply(2,3)==6

'''Test for Dividing Function'''
def test_divide():
    assert Official_Calculator.divide(4,2)==2